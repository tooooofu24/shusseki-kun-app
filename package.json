{
	"name": "shusseki-kun",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"export": "next build && next export",
		"eslint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
		"prettier:fix": "prettier --check --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md,mdx}'",
		"tsc": "tsc",
		"format": "npm run eslint:fix && npm run prettier:fix",
		"test": "jest --watch",
		"prepare": "husky install",
		"openapi:generate": "rimraf openapi/api-client && openapi-generator-cli generate -i openapi/scheme.yaml -g typescript-fetch -o openapi/api-client --custom-generator=openapi-generator-cli.jar --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.4.4",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@next/font": "13.0.7",
		"@openapitools/openapi-generator-cli": "^2.5.2",
		"@types/node": "18.11.17",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.9",
		"date-fns": "^2.29.3",
		"eslint": "8.30.0",
		"eslint-config-next": "13.0.7",
		"firebase": "^9.15.0",
		"framer-motion": "^7.10.3",
		"jotai": "^1.12.0",
		"next": "13.0.7",
		"phosphor-react": "^1.4.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-hook-form": "^7.41.0",
		"react-papaparse": "^4.1.0",
		"typescript": "4.9.4",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"babel-jest": "^29.3.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"react-test-renderer": "^18.2.0",
		"rimraf": "^3.0.2",
		"stylelint": "^14.16.0",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-recommended-scss": "^8.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.3.0"
	},
	"lint-staged": {
		"*.css": "stylelint --fix",
		"*.{js,jsx,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write",
			"tsc --noEmit"
		]
	}
}
